<div class="well well-lg">
	<form role="form" action="#" class="form-inline buyer-info" method="POST">
		<div class="form-group">
			<label for="firstName">First Name</label>
			<input type="text" class="form-control" value="{{data.name.givenName}}" id="firstName" placeholder="First Name">
		</div>

		<div class="form-group">
			<label for="firstName">Last Name</label>
			<input type="text" class="form-control" id="lastName" value="{{data.name.familyName}}" placeholder="Last Name">
		</div>

		<div class="form-group">
			<label for="emailAddress">Email Address</label>
			<input type="email" class="form-control" value="{{data.email}}" id="emailAddress" placeholder="Enter email">
		</div>

		<div class="form-group">
			<label for="ssn">SSN show</label>
			<input type="text" class="form-control ssn-input" id="ssn" placeholder="">
			-
			<input type="text" class="form-control ssn-input" id="ssn" placeholder="">
			-
			<input type="text" class="form-control ssn-input" id="ssn" placeholder="">
		</div>

		<div class="form-group">
			<label for="address">Address</label>
			<textarea rows="3" class="form-control" id="address" placeholder="Address">{{data.address}}</textarea>
		</div>

		<div class="form-group">
			<label for="city">City</label>
			<input type="text" class="form-control" id="city" value="{{data.city}}" placeholder="City">
		</div>

		<div class="form-group">
			<label for="state">State</label>
			<select id="state" class="form-control">
				<option value="AL">Alabama</option>
